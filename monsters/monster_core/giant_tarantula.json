{
    "name": "Giant Tarantula",
    "type": "creature",
    "sections": [
        {
            "name": "Giant Tarantula",
            "type": "section",
            "text": "Tarantulas are ambush predators but will attack prey in the open.",
            "sources": [
                {
                    "type": "source",
                    "name": "Monster Core",
                    "link": {
                        "type": "link",
                        "name": "Monster Core pg. 321",
                        "alt": "Monster Core pg. 321",
