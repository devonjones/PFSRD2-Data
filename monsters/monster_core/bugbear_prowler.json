{
    "name": "Bugbear Prowler",
    "type": "creature",
    "sections": [
        {
            "name": "Bugbear Prowler",
            "type": "section",
            "text": "Bugbear prowlers specialize in the art of lurking in the shadows.",
            "sources": [
                {
                    "type": "source",
                    "name": "Monster Core",
                    "link": {
                        "type": "link",
                        "name": "Monster Core pg. 47",
                        "alt": "Monster Core pg. 47",
                        "game-obj": "Sources",
                        "aonid": 221
                    },
                    "page": 47,
                    "errata": {
                        "type": "link",
