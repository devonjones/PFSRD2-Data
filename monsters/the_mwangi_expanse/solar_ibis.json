                    "ability_type": "automatic",
                    "name": "Blinding Halo",
                    "traits": [
                        {
                            "name": "Arcane",
                            "type": "trait",
                            "sources": [
                                {
                                    "type": "source",
                                    "name": "Core Rulebook",
                                    "link": {
                                        "type": "link",
                                        "name": "Core Rulebook pg. 628",
                                        "alt": "Core Rulebook pg. 628",
                                        "href": "https://paizo.com/products/btq01y0k?Pathfinder-Core-Rulebook"
                                    },
                                    "page": 628,
                                    "errata": {
                                        "type": "link",
                                        "name": "4.0",
                                        "alt": "4.0",
                                        "game-obj": "Sources",
                                        "aonid": 1
                                    }
