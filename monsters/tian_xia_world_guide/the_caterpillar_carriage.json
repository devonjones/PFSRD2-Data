                {
                    "name": "Clockwork",
                    "type": "trait",
                    "edition": "legacy",
                    "sources": [
                        {
                            "type": "source",
                            "name": "Grand Bazaar",
                            "link": {
                                "type": "link",
                                "name": "Grand Bazaar pg. 132",
