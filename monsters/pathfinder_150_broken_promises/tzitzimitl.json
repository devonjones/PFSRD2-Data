                }
            },
            {
                "name": "Undead",
                "class": "creature_type",
                "text": "Once living, these creatures were infused after death with negative energy and soul-corrupting evil magic. When reduced to 0 Hit Points, an undead creature is destroyed. Undead creatures are damaged by positive energy, are healed by negative energy, and don\u2019t benefit from healing effects.",
                "type": "stat_block_section",
                "subtype": "trait",
                "link": {
                    "type": "link",
                    "name": "Undead",
                    "alt": "Undead",
                    "game-obj": "Traits",
                    "aonid": 160
                }
            }
