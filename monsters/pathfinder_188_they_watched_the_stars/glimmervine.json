{
    "name": "Glimmervine",
    "type": "creature",
    "sections": [
        {
            "name": "Glimmervine",
            "type": "section",
            "sections": [
                {
                    "name": "Moonrods",
                    "type": "section",
                    "text": "When harvested with a successful DC 18 Nature or Survival check, a glimmervine's blossoms can be used as 6 gp worth of raw materials for crafting *sunrods*. *Sunrods* made with these materials give off a cold, bluish glow that more closely resembles moonlight than sunlight.",
                    "sources": [
                        {
                            "type": "source",
                            "name": "Pathfinder #188: They Watched the Stars",
                            "link": {
                                "type": "link",
                                "name": "Pathfinder #188: They Watched the Stars pg. 86",
                                "alt": "Pathfinder #188: They Watched the Stars pg. 86",
                                "game-obj": "Sources",
                                "aonid": 194
                            },
                            "page": 86
                        }
                    ],
                    "subtype": "sidebar",
                    "sidebar_type": "treasure_and_rewards",
                    "sidebar_heading": "Treasure and Rewards",
                    "image": {
                        "type": "image",
                        "name": "Treasure and Rewards",
                        "image": "Sidebar_5_TreasureAndRewards.png"
                    },
                    "links": [
                        {
                            "type": "link",
                            "name": "Nature",
                            "alt": "Nature",
                            "game-obj": "Skills",
                            "aonid": 10
                        },
                        {
                            "type": "link",
                            "name": "Survival",
                            "alt": "Survival",
                            "game-obj": "Skills",
                            "aonid": 16
                        },
                        {
                            "type": "link",
                            "name": "sunrods",
