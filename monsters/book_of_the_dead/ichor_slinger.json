            "automatic_abilities": [
                {
                    "type": "stat_block_section",
                    "subtype": "ability",
                    "ability_type": "automatic",
                    "name": "Mystery Ingredients",
                    "traits": [
                        {
                            "name": "Auditory",
                            "type": "trait",
                            "sources": [
                                {
                                    "type": "source",
                                    "name": "Core Rulebook",
                                    "link": {
                                        "type": "link",
                                        "name": "Core Rulebook pg. 629",
                                        "alt": "Core Rulebook pg. 629",
                                        "href": "https://paizo.com/products/btq01y0k?Pathfinder-Core-Rulebook"
                                    },
                                    "page": 629,
                                    "errata": {
                                        "type": "link",
                                        "name": "4.0",
                                        "alt": "4.0",
                                        "game-obj": "Sources",
                                        "aonid": 1
                                    }
                                }
                            ],
                            "game-obj": "Traits",
                            "game-id": "5cdf68d20776cac34c5945e6ea313fde",
                            "text": "Auditory actions and effects rely on sound. An action with the auditory trait can be successfully performed only if the creature using the action can speak or otherwise produce the required sounds. A spell or effect with the auditory trait has its effect only if the target can hear it. This applies only to sound-based parts of the effect, as determined by the GM. This is different from a sonic effect, which still affects targets who can't hear it (such as deaf targets) as long as the effect itself makes sound.",
                            "classes": [
                                "sense"
                            ],
                            "links": [
                                {
                                    "type": "link",
                                    "name": "sonic",
                                    "alt": "sonic",
                                    "game-obj": "Traits",
                                    "aonid": 147
                                },
                                {
                                    "type": "link",
                                    "name": "deaf",
                                    "alt": "deaf",
                                    "game-obj": "Conditions",
                                    "aonid": 8
                                }
                            ]
                        },
                        {
                            "name": "Aura",
                            "type": "trait",
                            "sources": [
                                {
                                    "type": "source",
                                    "name": "Core Rulebook",
                                    "link": {
                                        "type": "link",
                                        "name": "Core Rulebook pg. 629",
                                        "alt": "Core Rulebook pg. 629",
                                        "href": "https://paizo.com/products/btq01y0k?Pathfinder-Core-Rulebook"
                                    },
                                    "page": 629,
                                    "errata": {
                                        "type": "link",
                                        "name": "4.0",
                                        "alt": "4.0",
                                        "game-obj": "Sources",
                                        "aonid": 1
                                    }
                                }
                            ],
                            "game-obj": "Traits",
                            "game-id": "5aa4b65170eb0e79c2f82ed15cdeeba6",
                            "text": "An aura is an emanation that continually ebbs out from you, affecting creatures within a certain radius. Aura can also refer to the magical signature of an item or a creature with a strong alignment."
                        },
                        {
                            "name": "Occult",
                            "type": "trait",
                            "sources": [
                                {
                                    "type": "source",
                                    "name": "Core Rulebook",
                                    "link": {
                                        "type": "link",
                                        "name": "Core Rulebook pg. 634",
                                        "alt": "Core Rulebook pg. 634",
                                        "href": "https://paizo.com/products/btq01y0k?Pathfinder-Core-Rulebook"
                                    },
                                    "page": 634,
                                    "errata": {
                                        "type": "link",
                                        "name": "4.0",
                                        "alt": "4.0",
                                        "game-obj": "Sources",
                                        "aonid": 1
                                    }
                                }
                            ],
                            "game-obj": "Traits",
                            "game-id": "0815cac30474c8795eb23b0792923252",
                            "text": "This magic comes from the occult tradition, calling upon bizarre and ephemeral mysteries. Anything with this trait is magical.",
                            "classes": [
                                "tradition"
                            ],
                            "links": [
                                {
                                    "type": "link",
                                    "name": "magical",
                                    "alt": "magical",
                                    "game-obj": "Traits",
                                    "aonid": 103
                                }
                            ]
                        }
                    ],
                    "text": "60 feet. Creatures in the aura take a \u20132 status penalty to checks to <a aonid=\"44\" game-obj=\"Actions\">Identify Alchemy</a>, <a aonid=\"26\" game-obj=\"Actions\">Recall Knowledge</a> about alchemical subjects, or examine snake oil effects. When a creature first enters the area, if it hears the ichor slinger talking, it must attempt a <a aonid=\"142\" game-obj=\"Traits\">secret</a> Will save against the slinger's <a aonid=\"5\" game-obj=\"Skills\">Deception</a> DC. On a failure, it treats its next failed check to Identify Alchemy or Recall Knowledge about alchemical subjects in the next 24 hours as though it were a critical failure instead. This is an <a aonid=\"16\" game-obj=\"Traits\">auditory</a><a aonid=\"110\" game-obj=\"Traits\">misfortune</a> effect."
                }
            ]
        },
