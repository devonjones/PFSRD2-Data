                        "links": [
                            {
                                "type": "link",
                                "name": "Deception",
                                "alt": "Deception",
                                "game-obj": "Skills",
                                "aonid": 5
                            }
                        ],
                        "text": "The zrukbat assumes the shape of any Small object or ornamentation, although the zrukbat's mimicked objects usually have unrealistic or suspicious quirks. This doesn't change the zrukbat's size but can alter their coloration and visual appearance. They have an automatic result of 23 on Deception checks and DCs to pass as the mimicked object."
                    }
                },
                {
                    "name": "Go for the Eyes",
                    "type": "stat_block_section",
                    "subtype": "offensive_action",
                    "offensive_action_type": "ability",
                    "ability": {
                        "type": "stat_block_section",
                        "subtype": "ability",
                        "ability_type": "offensive",
                        "name": "Go for the Eyes",
                        "action": {
