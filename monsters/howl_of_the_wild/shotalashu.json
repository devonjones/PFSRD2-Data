        "interaction_abilities": [
            {
                "name": "Empathic Communication",
                "type": "stat_block_section",
                "subtype": "ability",
                "ability_type": "interaction",
                "text": "While a shotalashu can't speak, even telepathically, it understands simple commands in the languages it knows. It returns telepathic sensations of emotion to creatures touching it or the partner of its telepathic link at any distance."
            },
