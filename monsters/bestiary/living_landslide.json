{
    "name": "Living Landslide",
    "type": "creature",
    "sections": [
        {
            "name": "Living Landslide",
            "type": "section",
            "text": "Living landslides resemble humanoids made of earth and gravel.",
            "sources": [
                {
                    "type": "source",
                    "name": "Bestiary",
                    "link": {
                        "type": "link",
                        "name": "Bestiary pg. 146",
                        "alt": "Bestiary pg. 146",
