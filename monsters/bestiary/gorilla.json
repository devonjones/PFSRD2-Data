                {
                    "type": "source",
                    "name": "Bestiary",
                    "link": {
                        "type": "link",
                        "name": "Bestiary pg. 23",
                        "alt": "Bestiary pg. 23",
                        "href": "https://paizo.com/products/btq01y0m?Pathfinder-Bestiary"
                    },
                    "page": 23
                }
            ]
        },
        {
            "name": "Ape",
            "type": "section",
