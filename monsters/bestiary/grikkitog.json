        "interaction_abilities": [
            {
                "name": "Implant Core",
                "type": "stat_block_section",
                "subtype": "ability",
                "ability_type": "interaction",
                "action": {
                    "type": "stat_block_section",
                    "subtype": "action",
                    "name": "Three Actions"
