                            "type": "trait",
                            "sources": [
                                {
                                    "type": "source",
                                    "name": "Core Rulebook",
                                    "link": {
                                        "type": "link",
                                        "name": "Core Rulebook pg. 629",
                                        "alt": "Core Rulebook pg. 629",
