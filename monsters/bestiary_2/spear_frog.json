        {
            "name": "Spear Frog",
            "type": "section",
            "sources": [
                {
                    "type": "source",
                    "name": "Bestiary 2",
                    "link": {
                        "type": "link",
                        "name": "Bestiary 2 pg. 121",
                        "alt": "Bestiary 2 pg. 121",
