                        "traits": [
                            {
                                "name": "Nonlethal",
                                "type": "trait",
                                "sources": [
                                    {
                                        "type": "source",
                                        "name": "Core Rulebook",
                                        "link": {
                                            "type": "link",
                                            "name": "Core Rulebook pg. 283",
